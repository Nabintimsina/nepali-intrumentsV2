# Example environment variables
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_API_TIMEOUT=10000

# Audio/Media Storage
VITE_MEDIA_BASE_URL=https://your-cdn.com/media
VITE_AUDIO_BASE_URL=https://your-cdn.com/audio
VITE_MODEL_BASE_URL=https://your-cdn.com/models

# Authentication
VITE_JWT_SECRET=your-secret-key-here
VITE_JWT_EXPIRES_IN=7d

# Third-party Services
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_GOOGLE_CLIENT_ID=your-google-oauth-client-id

# Analytics
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Feature Flags
VITE_ENABLE_3D_VIEWER=true
VITE_ENABLE_SOCIAL_LOGIN=true
VITE_ENABLE_COMMENTS=false

# Development
VITE_DEBUG_MODE=false
